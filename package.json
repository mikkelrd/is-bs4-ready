    "zone.js": "^0.6.23",
    "angular2-template-loader": "^0.4.0",
    "autoprefixer": "^6.3.4",
    "codelyzer": "0.0.25",
    "cssnano": "^3.7.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
